{
    "name": "banditconsult/laravel-db-sync",
    "description": "Database synchronization for Laravel",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "banditconsult\\DbSync\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "banditconsult\\DbSync\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "banditconsult\\DbSync\\DbSyncServiceProvider"
            ]
        }
    }
}